    <style>
        .loadscreen {
            text-align: center;
            position: fixed;
            width: 100%;
            left: 0;
            right: 0;
            margin: auto;
            top: 0;
            height: 100vh;
            background: #ffffff;
            opacity: 0.7;
            z-index:2000;
        }

            .loadscreen .loader {
                position: absolute;
                top: calc(50vh - 50px);
                left: 0;
                right: 0;
                margin: auto;
            }

            .loadscreen .logo {
                display: inline-block !important;
                width: 80px;
                height: 80px;
            }
    </style>


	
